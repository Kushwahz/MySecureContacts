<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wordpress.helpmevishal.mycontacts">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login_SignUp"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"
            android:label="Login"
            android:screenOrientation="portrait"/>
        <activity android:name=".SignUp"
            android:label="SignUp"
            android:screenOrientation="portrait"/>
        <activity android:name=".Menu"
            android:screenOrientation="portrait"/>
        <activity android:name=".ViewContacts"
            android:label=""
            android:screenOrientation="portrait"/>
        <activity android:name=".AddContacts"
            android:label="Add New Contact"
            android:screenOrientation="portrait"/>
        <activity android:name=".EditContacts"
            android:label="Edit Contact"
            android:screenOrientation="portrait"/>
        <activity android:name=".DeleteContacts"
            android:label="Delete Contact"
            android:screenOrientation="portrait"/>
        <activity android:name=".SearchContact"
            android:label="Search Contact"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>